{
  "manifest_version": 2,
  "name": "AC Turnip Exchange Filter",
  "version": "0.2",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "https://turnip.exchange/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}